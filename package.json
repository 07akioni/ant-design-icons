{
  "name": "@ant-design/icons",
  "version": "0.1.2-alpha.8",
  "description": "Ant Design Icons",
  "module": "es/index.js",
  "main": "lib/index.js",
  "sideEffects": false,
  "repository": "https://github.com/ant-design/ant-design-icons",
  "contributors": [
    "HeskeyBaozi <hezhiyu233@foxmail.com>"
  ],
  "files": [
    "es",
    "lib",
    "svg"
  ],
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:lib": "rimraf lib && tsc",
    "build:es": "rimraf es && tsc --outDir es --module esnext",
    "generate": "cross-env TS_NODE_PROJECT=build/tsconfig.json node --require ts-node/register build/generateIcons.ts",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' 'site/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "start": "rimraf _site && cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js",
    "clean": "rimraf _site es lib src/manifest.ts src/svgs.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@svgr/core": "^2.1.1",
    "@types/globby": "^8.0.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.115",
    "@types/node": "^10.5.5",
    "@types/parse5": "^5.0.0",
    "@types/react": "^16.4.7",
    "@types/react-router": "3.0.15",
    "@types/rimraf": "^2.0.2",
    "@types/svgo": "^1.0.1",
    "antd": "^3.8.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "bisheng": "^0.28.0",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "globby": "^8.0.1",
    "jest": "^23.4.2",
    "lodash": "^4.17.10",
    "mobx": "^5.0.4",
    "mobx-react": "^5.2.5",
    "parse5": "^5.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rimraf": "^2.6.2",
    "styled-components": "^3.4.4",
    "svgo": "^1.0.5",
    "ts-jest": "^23.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "3.x",
    "webpackbar": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./test/tsconfig.json"
      }
    },
    "testURL": "http://localhost",
    "testPathIgnorePatterns": [
      "<rootDir>/packages/",
      "<rootDir>/node_modules/"
    ]
  }
}
