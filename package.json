{
  "name": "@ant-design/icons",
  "version": "0.1.2-alpha.1",
  "description": "Ant Design Icons",
  "module": "es/index.js",
  "main": "lib/index.js",
  "sideEffects": false,
  "repository": "https://github.com/ant-design/ant-design-icons",
  "contributors": [
    "HeskeyBaozi <hezhiyu233@foxmail.com>"
  ],
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:lib": "rimraf lib && tsc",
    "build:es": "rimraf es && tsc --outDir es --module esnext",
    "generate": "cross-env TS_NODE_PROJECT=build/tsconfig.json node --require ts-node/register build/generateIcons.ts",
    "test": "npm run test:unit",
    "test:unit": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha --require espower-typescript/guess test/**/*.ts -t 5000"
  },
  "devDependencies": {
    "@svgr/core": "^2.1.1",
    "@types/globby": "^8.0.0",
    "@types/lodash": "^4.14.115",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.5",
    "@types/parse5": "^5.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/svgo": "^1.0.1",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "espower-typescript": "^9.0.0",
    "globby": "^8.0.1",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "parse5": "^5.0.0",
    "power-assert": "^1.6.0",
    "rimraf": "^2.6.2",
    "svgo": "^1.0.5",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  }
}
