{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true,
    "es6": true
  },
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": ["plugin:vue/recommended"],
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "no-var": "error",
    "no-unused-vars": "warn",
    "camelcase": "off",
    "eol-last": "off",
    "semi": ["error", "always"],
    "space-before-function-paren": "off"
  }
}
